cess\";\nimport SuccessRequest from \"./pages/Success/success-request\";\nimport \"./style.scss\"\nconst Layout = () => {\n  return(\n    <>\n      <Outlet></Outlet>\n      <Footer></Footer>\n    </>\n  );\n\n};\nconst router = createBrowserRouter([\n  {\n    path: \"/\",\n    element:<Layout></Layout>,\n    children:[\n      {\n        path:\"/\",\n        element: <Home></Home>\n      },\n    \n      {\n        path: \"/customer\",\n        element: <Customer></Customer>\n      },\n      {\n        path: \"/success\",\n        element: <Success></Success>\n      },\n      {\n        path: \"/success-request\",\n        element: <SuccessRequest></SuccessRequest>\n      },\n      {\n        path: \"/maker\",\n        element: <Maker></Maker>\n      },\n      {\n        path: \"/submission/:id\",\n        element: <Submission></Submission>\n      }\n    \n    ]\n    \n  }\n  \n]);\nfunction App() {\n  return (\n    <div>\n      <RouterProvider router={router}/>\n    </div>\n  );\n}\n\n\nexport default App;\n",[],"/Users/vitto/Documents/PERSONAL/intern/user-stories/client/src/pages/Footer/Footer.jsx",[],[],"/Users/vitto/Documents/PERSONAL/intern/user-stories/client/src/pages/Success/Success.jsx",[],[],"/Users/vitto/Documents/PERSONAL/intern/user-stories/client/src/pages/Maker/Maker.jsx",["91","92"],[],"import React, { useEffect } from \"react\";\nimport \"./maker.scss\";\n// import { useState } from 'react'\nimport axios from \"axios\";\nimport {Link, useLocation } from \"react-router-dom\";\n// import { Link, useLocation, useNavigate } from \"react-router-dom\";\nimport { useState } from \"react\";\n\nconst Maker = () => {\n\n  const [inputs, setInputs] =  useState({\n    state_name:\"%\",\n    post_code:\"%\",\n    clothing_type:\"%\" //default value\n  })\n  const [post, setPosts] = useState([]);\n\n  const handleChangeF = e =>{\n    setInputs(prev=>({...prev, [e.target.name]:e.target.value}))\n\n   \n  }\n  const handleSubmit =async (e) =>{\n    try {\n      e.preventDefault();\n      \n      const res = await axios.post(`/posts/filter`, inputs);\n      setPosts(res.data);\n \n    } catch (err) {\n      console.log(err);\n    }\n  \n    \n  }\n\n  useEffect(() => {\n    const fetchData = async () => {\n      try {\n        const res = await axios.get(`/posts/`);\n\n        setPosts(res.data);\n    \n      } catch (err) {\n        console.log(err);\n      }\n    };\n    fetchData();\n  }, []);\n  return (\n    <div className=\"container-job\">\n      <form>\n        <input type=\"text\" onChange={handleChangeF} className=\"filter-input\"  name= \"state_name\" placeholder=\"State Name\"></input>\n        <input type=\"number\" onChange={handleChangeF} className=\"filter-input\" name= \"post_code\" placeholder=\"Post Code\"></input>\n        <select className=\"filter-input\" name=\"clothing_type\" onChange={handleChangeF}>\n              <option  value=\"\" >Type Of Clothing</option>\n              <option  value=\"Sari\" >Sari</option>\n              <option  value=\"Blouse\" >Blouse</option>\n              <option value=\"Dress\">Dress</option>\n              <option value=\"Ethnic Wear\">Ethnic Wear</option>\n        </select>\n       \n        \n        \n        <input type=\"button\" onClick={handleSubmit} className=\"filter-butt\"value=\"FILTER\"></input>\n      \n\n        \n      </form>\n      <div className=\"jobs\" id=\"jobs\">\n        {post.map((d) => {\n          return (\n            <div className=\"jobs__item\">\n              <div className=\"jobs__column jobs__column--left\">\n                <img src=\"./asset/Cloth.png\" alt=\"\" className=\"jobs__img\" />\n\n                <div className=\"jobs__info\">\n                  <span className=\"jobs__number\">JOB ID RECORD # {d.id}</span>\n                  <div className=\"text-cont\">\n                  <p className=\"jobs__details-label\">Name</p>\n                  <p className=\"jobs__details-item\">\n                    :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{d.first_name} {d.last_name}\n                  </p>\n                  </div>\n                  \n                  <br></br>\n\n                  <div className=\"text-cont\">\n                  <p className=\"jobs__details-label\">Type Of Clothing</p>\n                  <p className=\"jobs__details-item\">:&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{d.clothing_type}</p>\n                  </div>\n\n                  <br></br>\n                  <div className=\"text-cont\">\n                  <p className=\"jobs__details-label\">Location</p>\n                  <p className=\"jobs__details-item\">\n                    :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{d.street_num} {d.streetname},{d.state_name} {d.post_code}\n                  </p>\n                  </div>\n\n                  <br></br>\n                  <div className=\"text-cont\">\n                  <p className=\"jobs__details-label\">Submission</p>\n                  <p className=\"jobs__details-item\">\n                    :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{d.num_quotation} submissions</p>\n                  </div>\n            \n                  <br></br>\n                  <br></br>\n                  {/* <p className=\"jobs__details-item\">{d.description}</p> */}\n                </div>\n              </div>\n              <div className=\"button-container\">\n                \n                  <a href={`/submission/${d.id}`} className=\"job_button-sub\">\n                    <i className=\"job-but icon_img uil uil-user\"></i>\n\n                    <h5 className=\"job-but\">VIEW JOBS</h5>\n                  </a>\n      \n      \n              </div>\n            </div>\n          );\n        })}\n      </div>\n    </div>\n  );\n};\n\nexport default Maker;\n",[],"/Users/vitto/Documents/PERSONAL/intern/user-stories/client/src/pages/Customer/Customer.jsx",["93"],[],"import React from 'react'\nimport { useState } from 'react'\nimport {Link, useNavigate} from 'react-router-dom'\nimport \"./Customer.scss\"\nimport axios fro